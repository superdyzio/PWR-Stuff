#include "BlokProstujacy.hh"

/*!
 *\file
 *\brief	Plik zawierający definicję wybranych metod z klasy BlokProstujacy.
*/

bool BlokProstujacy::WyliczSygnalWyj(const Sygnal& Syg) {
	ZmienSygnalWyj(Syg.WezCzas(),abs(Syg.WezWartoscSyg()));
	cout << "bb";	
	return true;
}
